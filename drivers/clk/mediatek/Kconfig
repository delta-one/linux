config COMMON_CLK_MEDIATEK_FHCTL
	bool "clock driver for MediaTek FHCTL hardware control"
	select COMMON_CLK_MEDIATEK
	help
	  This driver supports MediaTek frequency hopping and
	  spread spectrum clocking features.

